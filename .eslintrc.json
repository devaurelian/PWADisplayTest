{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:all",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "func-style": "off",
        "space-before-function-paren": "off",
        "padded-blocks": "off",
        "one-var": "off",
        "max-len": ["error", 120],
        "array-element-newline": "off",
        "arrow-parens": ["error", "as-needed"]
    }
}