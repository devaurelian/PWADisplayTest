{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:all",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
        "array-element-newline": "off",
        "arrow-parens": ["error", "as-needed"],
        "func-style": "off",
        "init-declarations": "off",
        "linebreak-style": ["error", "windows"],
        "max-len": ["error", 120],
        "multiline-comment-style": "off",
        "object-curly-spacing": "off",
        "one-var": "off",
        "padded-blocks": "off",
        "space-before-function-paren": "off"
    }
}